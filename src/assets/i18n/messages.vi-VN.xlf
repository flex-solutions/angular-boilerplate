<?xml version="1.0" encoding="UTF-8" ?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file source-language="en" datatype="plaintext" original="ng2.template">
    <body>
      <!-- Nav Bar -->
      <trans-unit id="layout-navbar-anchor-changepassword" datatype="html">
        <source>Change Password</source>
        <target>Đổi Mật Khẩu</target>
      </trans-unit>
      <trans-unit id="layout-navbar-anchor-logout" datatype="html">
        <source>Sign Out</source>
        <target>Đăng Xuất</target>
      </trans-unit>

      <!-- Side Bar -->
      <trans-unit id="layout-sidebar-anchor-dashboard" datatype="html">
        <source>Dashboard</source>
        <target>Trang Chủ</target>
      </trans-unit>
      <trans-unit id="layout-sidebar-anchor-menu" datatype="html">
        <source>Menu</source>
        <target>Thực Đơn</target>
      </trans-unit>
      <trans-unit id="layout-sidebar-anchor-user-management" datatype="html">
        <source>User Management</source>
        <target>Quản Lý User</target>
      </trans-unit>

      <!--Create user-->
      <trans-unit id="user-create_user-h4-create_new_user_title" datatype="html">
        <source>Create New User</source>
        <target>Tạo User Mới</target>
      </trans-unit>
      <trans-unit id="user-create_user-h4-create_new_user_description" datatype="html">
        <source>Create a new user account to access SNOB CMS</source>
        <target>Tạo user mới truy cập hệ thống SNOB CMS</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-email_field" datatype="html">
        <source>Email</source>
        <target>Địa chỉ email</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-fullname_field" datatype="html">
        <source>Full name</source>
        <target>Tên đầy đủ</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-username_field" datatype="html">
        <source>Username</source>
        <target>Tên đăng nhập</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-password_field" datatype="html">
        <source>Password</source>
        <target>Mật khẩu</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-create_another_field" datatype="html">
        <source>Create another</source>
        <target>Tạo user khác</target>
      </trans-unit>
      <trans-unit id="user-create_user-button-submit" datatype="html">
        <source>Submit</source>
        <target>Submit</target>
      </trans-unit>
      <trans-unit id="user-create_user-button-cancel" datatype="html">
        <source>Cancel</source>
        <target>Hủy</target>
      </trans-unit>
      <trans-unit id="user-create_user-input-email_place_holder" datatype="html">
        <source>Enter email</source>
        <target>Nhập địa chỉ email</target>
      </trans-unit>
       <trans-unit id="user-create_user-input-fullname_place_holder" datatype="html">
        <source>Enter full name</source>
        <target>Nhập tên đầy đủ</target>
      </trans-unit>
      <trans-unit id="user-create_user-input-username_place_holder" datatype="html">
        <source>Enter username</source>
        <target>Nhập tên đăng nhập</target>
      </trans-unit>
      <trans-unit id="user-create_user-input-password_place_holder" datatype="html">
        <source>Enter password if required</source>
        <target>Nhập mật khẩu nếu cần thiết</target>
      </trans-unit>
      <trans-unit id="user-create_user-div-password_tooltip" datatype="html">
        <source>If you do not enter a password, one will be generated automatically</source>
        <target>Nếu bạn không nhập mật khẩu, hệ thống sẽ tự động tạo</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-validation_require_email" datatype="html"> 
        <source>Please provide email address</source>
        <target>Vui lòng cung cấp địa chỉ email </target>
      </trans-unit>
      <trans-unit id="user-create_user-label-validation_invalidEmail" datatype="html">
        <source>Email address is invalid</source>
        <target>Địa chỉ email không đúng</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-validation_uniqueEmail" datatype="html">
        <source>Email address {{0}} has already been taken</source>
        <target>Địa chỉ email {{0}} đã được sử dụng</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-validation_requireFullname" datatype="html">
        <source>Please provide full name</source>
        <target>Vui lòng cung cấp tên đầy đủ</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-validation_requireUserName" datatype="html">
        <source>Please provide username</source>
        <target>Vui lòng nhập tên đăng nhập</target>
      </trans-unit>
      <trans-unit id="user-create_user-label-validation_uniqueUserName" datatype="html">
        <source>Username {{0}} has already been taken</source>
        <target>Tên đăng nhập {{0}} đã được sử dụng</target>
      </trans-unit>
      <trans-unit id="user-create_user-notification-create_user_successfully" datatype="html">
        <source>Create user successfully</source>
        <target>Tạo user thành công </target>
      </trans-unit>
      <!--End create user-->

      <!--Edit user-->
      <trans-unit id="user-edit_user-h4-edit_user_title" datatype="html">
        <source>Edit User</source>
        <target>Sửa User</target>
      </trans-unit>
      <trans-unit id="user-edit_user-h4-edit_user_description" datatype="html">
        <source>Edit access information of an user account</source>
        <target>Sửa thông tin truy cập của user</target>
      </trans-unit>
       <trans-unit id="user-edit_user-label-is_active" datatype="html">
        <source>Is Active</source>
        <target>Cho phép đăng nhập</target>
      </trans-unit>
       <trans-unit id="user-edit_user-button-update" datatype="html">
        <source>Update</source>
        <target>Cập Nhật</target>
      </trans-unit>
      <!--End edit user-->
    </body>
  </file>
