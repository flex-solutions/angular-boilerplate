        <target>Tên đăng nhập {{0}} đã được sử dụng</target>
      </trans-unit>
      <trans-unit id="user-create_user-notification-create_user_successfully" datatype="html">
        <source>Create user successfully</source>
        <target>Tạo user thành công </target>
      </trans-unit>
      <trans-unit id="user-create_user-label-branchId_field" datatype="html">
