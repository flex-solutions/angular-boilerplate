      <!--Login-->
      <trans-unit id="account-login-input-username_place_holder" datatype="html">
        <source>Username</source>
        <target>Tên đăng nhập</target>
      </trans-unit>
      <trans-unit id="account-login-input-password_place_holder" datatype="html">
        <source>Password</source>
        <target>Mật khẩu</target>
      </trans-unit>
      <trans-unit id="account-login-paragraph-copyright" datatype="html">
        <source>Copyright © 2018 SNOB. All rights reserved.</source>
        <target>Bản quyền © 2018 SNOB. Đã đăng ký bản quyền.</target>
      </trans-unit>
      <trans-unit id="account-login-button-sign_in" datatype="html">
        <source>SIGN IN</source>
        <target>ĐĂNG NHẬP</target>
      </trans-unit>
      <trans-unit id="account-login-validation-emptyUsername" datatype="html">
        <source>Please enter your username</source>
        <target>Vui lòng nhập tên đăng nhập</target>
      </trans-unit>
      <trans-unit id="account-login-validation-emptyPassword" datatype="html">
        <source>Please enter your password</source>
        <target>Vui lòng nhập mật khẩu</target>
      </trans-unit>
